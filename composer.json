{
    "name": "hexlet/code",
    "description": "gendiff",
    "type": "project",
    "license": "mit",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hexlet\\Code\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^7.1",
        "doctrine/collections": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "4.x-dev",
        "phpunit/phpunit": "^13.0@dev"
    }
}
